{
  "name": "@vsol-admin/api",
  "version": "0.1.0",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "db:migrate": "tsx scripts/safe-migrate.ts",
    "db:migrate:unsafe": "tsx src/db/migrate.ts",
    "db:push": "drizzle-kit push:sqlite",
    "db:init": "tsx scripts/safe-init.ts",
    "db:init:unsafe": "tsx src/db/init-schema.ts",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx src/db/seed.ts",
    "db:generate": "drizzle-kit generate:sqlite",
    "db:backup": "node scripts/backup-db.js",
    "db:backup:list": "node scripts/verify-backup.js",
    "user:change-password": "tsx scripts/change-password.ts",
    "create-consultant-accounts": "tsx scripts/create-consultant-accounts.ts",
    "kill-port": "powershell -Command \"$port = $env:PORT; if (-not $port) { $port = 2020 }; $process = Get-NetTCPConnection -LocalPort $port -ErrorAction SilentlyContinue | Select-Object -ExpandProperty OwningProcess -First 1; if ($process) { Write-Host \\\"Killing process $process on port $port...\\\"; taskkill /F /PID $process } else { Write-Host \\\"No process found on port $port\\\" }\"",
    "kill-all-ports": "powershell -Command \"@(2020,2021) | ForEach-Object { $port = $_; $process = Get-NetTCPConnection -LocalPort $port -ErrorAction SilentlyContinue | Select-Object -ExpandProperty OwningProcess -First 1; if ($process) { Write-Host \\\"Killing process $process on port $port...\\\"; taskkill /F /PID $process } else { Write-Host \\\"No process on port $port\\\" } }\""
  },
  "dependencies": {
    "@journeyapps/sqlcipher": "^5.3.1",
    "@libsql/client": "^0.4.0",
    "@types/file-type": "^10.9.3",
    "@types/jspdf": "^2.0.0",
    "@types/pdfkit": "^0.17.4",
    "@types/supertest": "^6.0.3",
    "@vsol-admin/shared": "workspace:*",
    "argon2": "^0.31.2",
    "axios": "^1.13.1",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^12.4.1",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "date-fns": "^3.6.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.29.0",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "file-type": "^21.1.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.3",
    "multer": "^2.0.2",
    "node-cron": "^3.0.3",
    "pdfkit": "^0.17.2",
    "resend": "^6.6.0",
    "supertest": "^7.1.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.10.5",
    "@types/node-cron": "^3.0.11",
    "@vitest/ui": "^4.0.4",
    "better-sqlite3": "^12.4.1",
    "drizzle-kit": "^0.20.7",
    "mammoth": "^1.6.0",
    "tsx": "^4.6.2",
    "typescript": "^5.2.2",
    "vitest": "^4.0.4"
  }
}
